{
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["ESNext", "dom"],
    "types": ["cypress", "node"],
    "strict": true,
    "module": "ESNext",
    "noEmit": true,
    "moduleResolution": "node",
    "downlevelIteration": true,
    "typeRoots": ["../node_modules/@types"],
    "allowJs": true,
    "skipLibCheck": true,
    "noImplicitAny": true,
    "strictPropertyInitialization": false,
    "resolveJsonModule": true,
    "esModuleInterop": true
  },
  "include": ["**/*.ts"],
  "exclude": ["node_modules"]
}
